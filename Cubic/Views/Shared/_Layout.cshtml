<script type="text/javascript">
        function ChangePassword() {
            var url = '@Url.Action("Index", "ChangePassword")';
            $("#ChangePass").load(url);
        }

        function ChangeMenue() {
            if ($('#body').hasClass("minified")) {
                $.cookie("MenueStyle", 0);
            }
            else {
                $.cookie("MenueStyle", 1);
            }
        }
        function AlertSuccess(msg) {
            $.smallBox({
                title: "Success",
                content: "<p style='color:white'>" + msg + "</p>",
                color: "rgb(115, 158, 115)",
                timeout: 8000,
                icon: "fa fa-bell swing animated"
            });
        }
        function AlertError(msg) {
            $.smallBox({
                title: "Failed",
                content: "<p style='color:white'>" + msg + "</p>",
                color: "#b94a48",
                timeout: 8000,
                icon: "fa fa-bell swing animated"
            });
        }
        $(document).on("keydown", function (e) {
            if (e.which === 8 && !$(e.target).is("input, textarea")) {
                e.preventDefault();
            }
        });

        window.onload = function () { // When the page has finished loading.
            $(".nested").each(function () { // Check every "nested" class
                if ($(this).children().length == 0) { // If this nested class has no children
                    //$("#ReportsList").hide(); // This will hide it, but not alter the layout
                    // $(this).css("display", "none"); // This will alter the layout
                    $(this).parent().hide();
                } else {
                   // $("#ReportsList").show();
                    //$(this).css("display", "none"); // This will alter the layout
                }
            }
            );
    }
</script>
<script type="text/javascript">
    function ExecuteAction(URL, _data) {
        var res = "";
        $.ajax({
            url: URL,
            data: _data,
            cache: false,
            type: "GET",
            async: false,
            success: function (data) {
                res = data;
            },
            error: function () {
                location.reload();
            }
        });
        return res;
    }

    $(document).on("click", ".serverurl", function () {
        var _url = '@Url.Action("serverurl", "Home")';

        var serverurl = $(this).attr("data-link");
        alert(serverurl);


        $.ajax({
            url: _url,
            data: {},
            cache: false,
            type: "GET",
            async: false,
            success: function (data) {
                alert(serverurl + data);
                window.open(serverurl + data, "_blank");
            },
            error: function (error) {
                AlertError(error);
            }
        });
    });
    $(document).ready(function () {
        // var path = window.location.pathname.substring(1);
        var path = window.location.pathname;
        // $('.nav>li>a[href="' + path + '"]').parent().addClass('active');
        $('a[href="' + path + '"]').parent().addClass('active');
        $('a[href="' + path + '"]').parent().parent().css("display","block");
        //display: block;
    });
</script>


