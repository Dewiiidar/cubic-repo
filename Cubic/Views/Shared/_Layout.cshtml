@if (TempData["Success"] != null)
{
    <script>
        $(document).ready(function () {
            $.smallBox({
            title: "Success",
            content: '@TempData["Success"]',
            color: "rgb(115, 158, 115)",
            timeout: 8000,
            icon: "fa fa-bell swing animated"
            });
        });
    </script>
}
else if (TempData["Error"] != null)
{
    <script>
        $(document).ready(function () {
            $.smallBox({
                title: "Failed",
                content: '@TempData["Error"]',
                color: "#b94a48",
                timeout: 8000,
                icon: "fa fa-bell swing animated"
            });
        });
    </script>
}

